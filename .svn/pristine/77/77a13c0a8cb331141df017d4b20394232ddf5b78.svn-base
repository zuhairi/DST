//object instances of these classes represent the summary results of a SINGLE component (link or node) for a specific type of simulation.


package com.watergrid.dst.client.dataobjects;

public class LinkHydraulicResult {
	
	String linkId;
	float minFlow,maxFlow, avgFlow;
	float minVelocity,maxVelocity, avgVelocity;
	float minHeadloss,maxHeadloss, avgHeadloss;
	
	
	public LinkHydraulicResult(String linkId, float minFlow, float maxFlow,float avgFlow, float minVelocity, float maxVelocity,
								float avgVelocity, float minHeadloss, float maxHeadloss,float avgHeadloss) {

		this.linkId = linkId;
		this.minFlow = minFlow;
		this.maxFlow = maxFlow;
		this.avgFlow = avgFlow;
		this.minVelocity = minVelocity;
		this.maxVelocity = maxVelocity;
		this.avgVelocity = avgVelocity;
		this.minHeadloss = minHeadloss;
		this.maxHeadloss = maxHeadloss;
		this.avgHeadloss = avgHeadloss;
	}


	public String getLinkId() {
		return linkId;
	}


	public float getMinFlow() {
		return minFlow;
	}


	public float getMaxFlow() {
		return maxFlow;
	}


	public float getAvgFlow() {
		return avgFlow;
	}


	public float getMinVelocity() {
		return minVelocity;
	}


	public float getMaxVelocity() {
		return maxVelocity;
	}


	public float getAvgVelocity() {
		return avgVelocity;
	}


	public float getMinHeadloss() {
		return minHeadloss;
	}


	public float getMaxHeadloss() {
		return maxHeadloss;
	}


	public float getAvgHeadloss() {
		return avgHeadloss;
	}
	
	

}
