package com.watergrid.dst.client.model;

import java.util.ArrayList;

public class Piping { //Pipe name is used by Java
	
	int index;
	String id;
	double length;
	double diameter;
	double roughness;
	double minorLoss;
	String status;
	ArrayList<Coordinate> vertices;
	String startNodeId;
	String endNodeId;
	
	
	public Piping(){
		
	}
	
	public Piping(int index,String id,double length,double diameter,double roughness,double minorLoss,String status,ArrayList<Coordinate> vertices,String startNodeId,String endNodeId){
		this.index=index;
		this.id=id;
		this.length=length;
		this.diameter=diameter;
		this.roughness=roughness;
		this.minorLoss=minorLoss;
		this.status=status;
		this.vertices=vertices;	
		this.startNodeId=startNodeId;
		this.endNodeId=endNodeId;
	}
	
	//setters
	public void setIndex(int index){
		this.index=index;	
	}
	public void setId(String id){
		this.id=id;
	}
	public void setLength(double length){
		this.length=length;
	}
	public void setDiameter(double diameter){
		this.diameter=diameter;
	}
	public void setRoughness(double roughness){
		this.roughness=roughness;
	}
	public void setMinorLoss(double minorLoss){
		this.minorLoss=minorLoss;
	}
	public void setStatus(String status){
		this.status=status;
	}
	public void setVertices(ArrayList<Coordinate> vertices){
		this.vertices=vertices;
	}
	public void setStartNodeId(String startNodeId){
		this.startNodeId=startNodeId;
	}
	public void setEndNodeId(String endNodeId){
		this.endNodeId=endNodeId;
	}
	
	//getters
	public int getIndex(){
		return index;	
	}
	public String getId(){
		return id;
	}
	public double getLength(){
		return length;
	}
	public double getDiameter(){
		return diameter;
	}
	public double getRoughness(){
		return roughness;
	}
	public double getMinorLoss(){
		return minorLoss;
	}
	public String getStatus(){
		return status;
	}
	public ArrayList<Coordinate> getVertices(){
		return vertices;
	}
	public String getStartNodeId(){
		return startNodeId;
	}
	public String getEndNodeId(){
		return endNodeId;
	}

}
